            }elseif (!$record['VERIFIED1'] && $record['VERIFIED2']) {
                $wrapper = entity_metadata_wrapper('asotry_form',$id1);
                $wrapper->delete();
                if(function_exists('drush_print')){
                    drush_print('Form ID : '.$id1.' de type : '.$record['TYPE1'].' Supprimé');
