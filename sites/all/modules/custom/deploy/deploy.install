        }
        if($record['TYPE1'] == 'fd16'){
            $id1 = $record['ID1'];
            $id2 = $record['ID2'];
            // raha efa verifié ilay voalohany dia fafana ilay faharoa
            if($record['VERIFIED1'] && !$record['VERIFIED2']){
                $wrapper = entity_metadata_wrapper('asotry_form',$id2);
                $wrapper->delete();
